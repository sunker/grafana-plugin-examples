{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "ignorePaths": [],
  "enabledManagers": ["npm"],
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "enabled": false
    },
    {
      "packagePatterns": ["@grafana/plugin-e2e"],
      "enabled": true
    },
    {
      "matchPackagePrefixes": ["@grafana/"],
      "enabled": true
    }
  ]
}
